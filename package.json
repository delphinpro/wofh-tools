{
    "private": true,
    "scripts": {
        "start": "npm run dev -- --watch",
        "dev": "cross-env BUNDLE=client NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "cross-env NODE_ENV=production npm run build:server && cross-env NODE_ENV=production npm run build:client",
        "build:client": "cross-env BUNDLE=client node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build:server": "cross-env BUNDLE=server node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
        "autoprefixer": "^9.8.6",
        "browser-sync": "^2.26.12",
        "browser-sync-webpack-plugin": "^2.2.2",
        "cross-env": "^7.0",
        "laravel-mix": "^5.0.1",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.20.1",
        "sass-loader": "^8.0.0",
        "vue-server-renderer": "^2.6.11",
        "vue-template-compiler": "^2.6.10"
    },
    "dependencies": {
        "@quasar/extras": "^1.9.5",
        "axios": "^0.19",
        "cxlt-vue2-toastr": "^1.1.1",
        "highcharts": "^8.1.2",
        "highcharts-vue": "^1.3.5",
        "quasar": "^1.14.0",
        "vue": "^2.6.11",
        "vue-router": "^3.4.3",
        "vuex": "^3.5.1"
    }
}
